{
  "expo": {
    "name": "NutritionAI",
    "slug": "nutrition-ai-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "nutritionai",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kuyos.nutritionaimobile"
    },
    "android": {
      "package": "com.kuyos.nutritionaimobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundImage": "./assets/adaptive-icon.png",
        "monochromeImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1A1D29"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "84c107db-b05c-4a43-9afb-bb9d3902632f"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}